{
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devPath": "http://localhost:5211",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Focuser",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "window": {
        "maximize": false,
        "unmaximize": false
      }
    },
    "bundle": {
      "active": true,
      "category": "Productivity",
      "copyright": "(C) TRIStudio 2023",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "studio.tri.focus.pro",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        
        "height": 700,
        "resizable": true,
        "title": "Focuser",
        "width": 500,
        "minHeight": 700,
        "maxHeight": 1000,
        "minWidth": 500,
        "maxWidth": 1000,
        "maximized": false
      }
    ]
  }
}
